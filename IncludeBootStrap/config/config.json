{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "bootstrap-accordion-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/bootstrapAccordion/BootstrapAccordionWebPart.js",
          "manifest": "./src/webparts/bootstrapAccordion/BootstrapAccordionWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {
    "jquery": {
      "path": "https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js",
      "globalName": "jquery"
      }, 
      "bootstrap": {
        "path": "https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js",
        "globalName": "bootstrap",
        "globalDependencies": ["jquery"]
        } 
  },
  "localizedResources": {
    "BootstrapAccordionWebPartStrings": "lib/webparts/bootstrapAccordion/loc/{locale}.js"
  }
}
